@{
}
@model Workfromhell.Models.db.Employee;
<form asp-action="Create">
    <div class="form-group">
        <label class="control-label" asp-for="Name">First Name</label>
        <input class="form-control" asp-for="Name" placeholder="John" required/>
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="Lastname">Last Name</label>
        <input class="form-control" asp-for="Lastname" placeholder="Mhadilopnopparatrachatanee" required/>
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="Age">Age</label>
        <input class="form-control" min="1" max="99" asp-for="Age" placeholder="10" required/>
    </div>
    <div class="form-group">
        <label class="control-label" asp-for="Joined">Joined Date</label>
        <input class="form-control" asp-for="Joined"/>
        <span asp-validation-for="Joined" class="text-danger"></span>
    </div>
    <input type="submit" value="Create" class="btn btn-primary"/>
  
    

</form>